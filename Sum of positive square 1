import java.util.Scanner;
public class SumOfPositiveSquares {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int sum = 0;
        for (int i = 0; i < n; i++) {
            int num = scanner.nextInt();
            if (num > 0) {
                sum += num * num;
            }
        }
        System.out.println(sum);

        scanner.close();
    }
}
